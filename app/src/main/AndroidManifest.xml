<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.mwami.myapplication">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher3"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="E-School">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".LoginActivity"
            android:label="Student > Login" />
        <activity
            android:name=".StudentLogin"
            android:label="Student > Login" />
        <activity
            android:name=".student_modules"
            android:label="@string/title_activity_student_modules"
            android:theme="@style/AppTheme.NoActionBar"></activity>
    </application>

</manifest>